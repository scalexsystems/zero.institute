    "eslint-plugin-vue": "^2.0.1",
    "function-bind": "^1.1.0",
    "glob": "latest",
    "html-webpack-plugin": "^2.28.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.5.0",
    "karma-chai-dom": "^1.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
