    "sifter": "^0.5.2",
    "socket.io-client": "^1.7.2",
    "tether": "^1.4.0",
    "vue": "^2.1.10",
    "vue-infinite-loading": "^2.0.0",
    "vue-resource": "^1.2.0",
    "vue-route": "^1.5.1",
