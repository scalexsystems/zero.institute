{
  "name": "Zero",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --hide-modules",
    "build:dll": "NODE_ENV=development webpack --progress --hide-modules --config tests/unit/dll.webpack.config.js",
    "build:clean": "rm -rf public/app",
    "predev": "yarn run io",
    "dev": "NODE_ENV=development webpack --watch --progress --hide-modules",
    "dev:components": "webpack-dashboard -- webpack-dev-server --config tests/unit/runner.js --open",
    "prehot": "yarn run io &",
    "hot": "node scripts/hot.js",
    "build:prod": "NODE_ENV=production webpack --env=prod --progress --hide-modules",
    "io": "laravel-echo-server start",
    "test": "yarn run lint:fix && yarn run test:unit && yarn run test:e2e",
    "test:e2e": "node tests/e2e/runner.js",
    "test:unit": "karma start tests/unit/karma.conf.js --single-run",
    "lint": "eslint --ext .js,.vue resources/assets/app tests/e2e tests/unit",
    "lint:fix": "eslint --fix --ext .js,.vue resources/assets/app",
    "preprod": "yarn version --no-git-tag-version",
    "prod": "./scripts/production.sh"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^1.0.2",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
