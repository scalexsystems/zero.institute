{
  "name": "Zero",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --hide-modules",
    "build:dll": "NODE_ENV=development webpack --progress --hide-modules --config tests/unit/dll.webpack.config.js",
    "predev": "yarn run io",
    "dev": "NODE_ENV=development webpack --watch --progress --hide-modules",
    "dev:components": "webpack-dashboard -- webpack-dev-server --config tests/unit/runner.js --open",
    "prehot": "yarn run io",
    "hot": "node scripts/hot.js",
    "build:prod": "NODE_ENV=production webpack --env=prod --progress --hide-modules",
    "io": "laravel-echo-server start &",
    "test": "yarn run lint:fix && yarn run test:unit && yarn run test:e2e",
    "test:e2e": "node tests/e2e/runner.js",
    "test:unit": "karma start tests/unit/karma.conf.js --single-run",
    "lint": "eslint --ext .js,.vue resources/assets/app tests/e2e tests/unit",
    "lint:fix": "eslint --fix --ext .js,.vue resources/assets/app",
    "preprod": "yarn version --no-git-tag-version",
    "prod": "./scripts/production.sh"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^1.0.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "chai-dom": "^1.4.3",
    "chromedriver": "^2.27.2",
    "cross-spawn": "latest",
    "eslint": "^3.15.0",
    "eslint-config-vue": "^2.0.1",
    "eslint-plugin-html": "^2.0.0-beta.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-vue": "^2.0.1",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.4.1",
    "karma-chai-dom": "^1.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.26",
    "karma-webpack": "^2.0.1",
    "laravel-mix": "^0.7.2",
    "mocha": "^3.2.0",
    "mocha-css": "^1.0.1",
    "nightwatch": "^0.9.12",
    "node-sass": "^4.5.0",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-cssnext": "^2.9.0",
    "rollup-loader": "^0.1.2",
    "rollup-plugin-babel": "^2.7.1",
    "sass-loader": "^5.0.0",
    "selenium-server": "^3.0",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "touch": "^1.0.0",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.3.0",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "Validator": "^1.0.3",
    "autosize": "^3.0.20",
    "bootstrap": "next",
    "bootstrap-for-vue": "^0.1.14",
    "debug": "^2.6.0",
    "es6-promise": "^4.0.5",
    "filesize": "^3.3.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
