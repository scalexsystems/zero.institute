    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "chai-dom": "^1.4.3",
    "chromedriver": "^2.28.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.27.3",
